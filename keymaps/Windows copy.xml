    <keyboard-shortcut first-keystroke="alt c" />
  </action>
  <action id=":cursive.repl.actions/run-top-sexp">
    <keyboard-shortcut first-keystroke="shift alt p" />
    <keyboard-shortcut first-keystroke="ctrl alt comma" second-keystroke="b" />
  </action>
  <action id=":cursive.testing.actions/run-ns-tests">
    <keyboard-shortcut first-keystroke="ctrl alt comma" second-keystroke="shift t" />
  </action>
  <action id=":cursive.testing.actions/run-single-test">
    <keyboard-shortcut first-keystroke="ctrl alt comma" second-keystroke="t" />
  </action>
  <action id="EditSource">
    <keyboard-shortcut first-keystroke="f4" />
    <keyboard-shortcut first-keystroke="ctrl alt comma" second-keystroke="d" />
  </action>
  <action id="ExpressionTypeInfo" />
  <action id="GotoFile">
    <keyboard-shortcut first-keystroke="shift ctrl n" />
    <keyboard-shortcut first-keystroke="shift ctrl p" />
  </action>
</keymap>